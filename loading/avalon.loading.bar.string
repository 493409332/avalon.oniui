{{MS_WIDGET_BALL}}
<v:oval style="{{MS_WIDGET_CSS_BALL}}position:absolute;BEHAVIOR:url(#default#VML)" stroked="false" 
	ms-attr-fillcolor="$config.color" {{MS_WIDGET_CSS_BINDINGS}}>
</v:oval>
{{MS_WIDGET_DIVIDER}}
<svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%">
	<circle style="{{MS_WIDGET_CSS_BALL}}" {{MS_WIDGET_CSS_BINDINGS}} 
	        ms-repeat="data" 
	        ms-attr-fill="$config.color">
	</circle>
</svg>
{{MS_WIDGET_TYPE}}
{{MS_WIDGET_SPIN}}
<v:oval stroked="true" filled="F" 
	ms-attr-strokecolor="$config.color" 
    ms-css-height="$config.widthInner+'px'" 
    ms-css-width="$config.widthInner+'px'" 
	ms-css-opacity="$config.opacity" 
	ms-attr-strokeweight="$config.width / 2 - $config.widthInner / 2" 
	style="position:absolute;z-index:2;behavior:url(#default#VML);"></v:oval>
<v:arc stroked="true" filled="F" 
    ms-attr-strokecolor="$config.color" 
    ms-attr-strokeweight="$config.width / 2 - $config.widthInner / 2" 
    style="position:absolute;z-index:3;behavior:url(#default#VML);text-indent:-1000px;overflow:hidden;" 
    startangle="data2[0]" 
    ms-css-height="$config.widthInner+'px'" 
    ms-css-width="$config.widthInner+'px'" 
    ms-attr-endangle="data2[1]">
	</v:arc>
{{MS_WIDGET_DIVIDER}}
<svg width="100%" height="100%" version="1.1" xmlns="http://www.w3.org/2000/svg">
	<path 
		ms-attr-d="data2.join('')" 
		ms-attr-stroke="$config.color" 
		ms-attr-stroke-width="$config.width / 2 - $config.widthInner / 2" 
		stroke-linejoin="round" fill="none"></path>
	<path stroke-linejoin="round" 
		ms-attr-d="data.join('')" 
		ms-attr-stroke-width="$config.width / 2 - $config.widthInner / 2" 
		ms-attr-stroke="$config.color" 
		ms-css-opacity="$config.opacity" 
		fill="none"></path>
	</svg>